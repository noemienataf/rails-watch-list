<h2><%= @list.name %></h2>
  <ul>
  <% @list.movies.each do |movie| %>
    <li>
      <% bookmark = Bookmark.find_by(list: @list, movie: movie) %>
      <%= movie.title %>
      <p><%= link_to "Delete", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Noemie ok ?"} %></p>
    </li>
  <% end %>
  </ul>
  <p><%= link_to "Add a movie", new_list_bookmark_path(@list)%></p>
